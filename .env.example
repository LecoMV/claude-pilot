# Claude Pilot Environment Configuration
# Copy this file to .env and fill in your values

# PostgreSQL Database Configuration
CLAUDE_PG_HOST=localhost
CLAUDE_PG_PORT=5433
CLAUDE_PG_USER=deploy
CLAUDE_PG_DATABASE=claude_memory
CLAUDE_PG_PASSWORD=your_secure_password_here

# Sentry Crash Reporting (optional but recommended)
# Get your DSN from https://sentry.io/settings/projects/YOUR_PROJECT/keys/
# Or use: pass show claude/sentry/dsn
SENTRY_DSN=
